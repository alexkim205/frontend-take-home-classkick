{
  "name": "classkick-frontend-takehome",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"vite\" \"kea-typegen watch --write-paths\" -n APP,KEA -c blue,green",
